
<% include ./partials/header %>


<h1>Add a new car to your collection</h1>

<form action="/cars" method="POST">
    <input type="text" name="car[make]" placeholder="Make">
    <input type="text" name="car[model]" placeholder="Model">
    <input type="text" name="car[image]" placeholder="image Link">
    <textarea name="car[description]" cols="30" rows="10"></textarea>
    <input type="submit">
</form>

